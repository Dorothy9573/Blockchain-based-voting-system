{
  "installCommand": "npm install --prefix client && npm install",
  "buildCommand": "npm run build --prefix client",
  "outputDirectory": "client/build",
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

{
  "version": 2,
  "installCommand": "npm install && npm install --prefix client",
  "buildCommand": "npm run build --prefix client",
  "outputDirectory": "client/build",
  
  "builds": [
    {
  
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
      "src": "/api/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}
